stack segment
dw   128  dup(0)
ends

code segment
;----print photos proc----;
PrintScr: mov bp,sp
push ax
push bx
push dx
push cx
push si

mov bx,[bp+2]
mov ah,0ch
mov cx,x
mov dx,y
add colsize,cx
add rowsize,dx
xx:    mov al,[bx]
int 10h

inc bx
inc cx
cmp cx,colsize
jne xx

xx1: mov cx,x
inc dx
cmp dx,rowsize
jae sofProc
jne xx
sofProc:  pop si
pop cx
pop dx
pop bx
pop ax
ret

;----clear screen proc----;
cls:
push ax
mov ax,3
int 10h
mov ah,0
mov al,13h ; set graphics  mode;VGA, 320*200, 256 colors
int 10h
pop ax
ret

;----kelet proc----;
keletstr:
Push bp
mov bp,sp
Push ax
Push si
mov ah,1
mov si,[bp+4]
islo1:
mov ah,1
int 21h
CMP al,13
JE ishem1
mov [si],al
inc si
JMP islo1
ishem1:
mov Byte Ptr[si],'$'
Pop si
Pop ax
Pop bp
ret 2

;----print string proc----;
printstr:
Push bp
mov bp,sp
Push ax
Push dx
mov ah,9
mov dx,[bp+4]
int 21h
Pop dx
Pop ax
Pop bp
ret 2


;----int to str proc----;
inttostr:push bp
mov bp,sp
push ax
push bx
push dx
push cx
mov cx,0
mov bx,[bp+4]
mov al,[bx]
mov bx,[bp+6]
mov ah,0
mov dl,10
i1:    cmp al,0
je i2
mov ah,0
div dl
push ax
inc cx
jmp i1
i2:    pop ax
add ah,30h
mov [bx],ah
inc bx
loop i2
mov byte ptr[bx],'$'
pop cx
pop dx
pop bx
pop ax
pop bp
ret 4

;----checking proc(if the words are similar)----;
check:
mov bp,sp
push bx
push si
push ax
mov si,[bp+2]
mov bx,[bp+4]
startch:   cmp [bx],byte ptr '$'
je check1
cmp [si],byte ptr '$'
je checkN
mov al,[bx]
cmp al,[si]
jne  checkN
inc bx
inc si
jmp startch

check1:  cmp [si],byte ptr '$'
je checkY

checkN:mov correct,0
jmp  sofch

checkY:mov correct,1

sofch: pop ax
pop si
pop bx
ret 4

;----pause proc----;
pause:push ax
push bx
push dx
mov ah,00h
int 1Ah
mov bx,dx

puseL1:int 1Ah
sub dx,bx
cmp dx,20
ja endPause
jmp puseL1

endPause:pop dx
pop bx
pop ax
ret

;----code----;
start:

mov ax,@data
mov ds,ax

mov ah,0
mov al,13h ; set graphics  mode;VGA, 320*200, 256 colors
int 10h


mov cx,ptiharow
mov ax,ptihacol
mov rowsize,cx
mov colsize,ax

mov bx,offset ptiha
push bx
call printscr

push offset key
call printstr

mov ah,1
int 21h
cmp al,' '
jne menuu


menuu:
call cls
mov cx,menurow
mov ax,menucol
mov rowsize,cx
mov colsize,ax

mov bx,offset menu
push bx
call printscr

mov ah,1
int 21h

cmp al,'1'
je newgame
cmp al,'2'
je help
cmp al,'3'
je exit


newgame:
call cls
mov cx,catrow
mov ax,catcol
mov rowsize,cx
mov colsize,ax

mov bx,offset cat
push bx
call printscr
mov bx,offset question
push bx
call printstr
q1: mov bx,offset answerr
push bx
call keletstr


push  offset answerr
push offset answer1
call check

cmp correct,1
jne no1
add res,5
call cls
push offset ok
call  printstr
jmp q2

no1: call cls
push offset error1
call printstr


q2:
call pause
call cls
mov cx,butterrow
mov ax,buttercol
mov rowsize,cx
mov colsize,ax

mov cx,offset butterfly
push cx
call printscr
mov bx,offset question
push bx
call printstr
mov bx,offset answerr
push bx
call keletstr

push  offset answerr
push offset answer2
call check

cmp correct,1
jne no2
add res,5
call cls
push offset ok
call  printstr
jmp q3

no2: call cls
push offset error2
call printstr


q3:
call pause
call cls
mov cx,heartrow
mov ax,heartcol
mov rowsize,cx
mov colsize,ax

mov cx,offset heart
push cx
call printscr
mov bx,offset question
push bx
call printstr
mov bx,offset answerr
push bx
call keletstr

push  offset answerr
push offset answer3
call check

cmp correct,1
jne no3
add res,5
call cls
push offset ok
call  printstr
jmp q4

no3: call cls
push offset error3
call printstr


q4:
call pause
call cls
mov cx,houserow
mov ax,housecol
mov rowsize,cx
mov colsize,ax

mov cx,offset house
push cx
call printscr
mov bx,offset question
push bx
call printstr
mov bx,offset answerr
push bx
call keletstr

push  offset answerr
push offset answer4
call check

cmp correct,1
jne no4
add res,5
call cls
push offset ok
call  printstr
jmp q5

no4: call cls
push offset error4
call printstr


q5:
call pause
call cls
mov cx,pencilrow
mov ax,pencilcol
mov rowsize,cx
mov colsize,ax

mov cx,offset pencil
push cx
call printscr
mov bx,offset question
push bx
call printstr
mov bx,offset answerr
push bx
call keletstr

push  offset answerr
push offset answer5
call check

cmp correct,1
jne no5
add res,5
call cls
push offset ok
call  printstr
jmp q6

no5: call cls
push offset error5
call printstr


q6:
call pause
call cls
mov cx,shirtrow
mov ax,shirtcol
mov rowsize,cx
mov colsize,ax

mov cx,offset shirt
push cx
call printscr
mov bx,offset question
push bx
call printstr
mov bx,offset answerr
push bx
call keletstr

push  offset answerr
push offset answer6
call check

cmp correct,1
jne no6
add res,5
call cls
push offset ok
call  printstr
jmp q7

no6: call cls
push offset error6
call printstr


q7:
call pause
call cls
mov cx,applerow
mov ax,applecol
mov rowsize,cx
mov colsize,ax

mov cx,offset apple
push cx
call printscr
mov bx,offset question
push bx
call printstr
mov bx,offset answerr
push bx
call keletstr

push  offset answerr
push offset answer7
call check

cmp correct,1
jne sof2
add res,5
call cls
push offset ok
call  printstr
jmp sof

no7: call cls
push offset error7
call printstr


sof: call cls
add res,5
mov bx,offset res
push bx
call inttostr
mov cx,offset score
push cx
call printstr
mov bx,offset res
push bx
call printstr
jmp exit

sof2:call cls
add res,5
mov bx,offset res
push bx
call inttostr
mov cx,offset score
push cx
call printstr
mov bx,offset res
push bx
call printstr
jmp exit


help:
call cls
mov bx,offset instra
push bx
call printstr

mov ah,1
int 21h

cmp al,' '
jne menuu



exit:

mov ax, 4c00h
int 21h

ends

end start




